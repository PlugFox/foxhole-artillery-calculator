<Window x:Class="foxhole_artillery_calculator.screens.Data"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:foxhole_artillery_calculator.screens"
        mc:Ignorable="d"
        Title="Прочность построек" Height="650" Width="550" FontSize="16" Background="Black" Foreground="LightGray" 
        WindowStyle="ToolWindow" SizeToContent="Height" MouseLeftButtonDown="WindowMouseLeftButtonDown"
        ResizeMode="NoResize">
    <x:Code>
        <![CDATA[            
                private void WindowMouseLeftButtonDown(object sender, MouseButtonEventArgs e)
                {
                    DragMove();
                }
            ]]>
    </x:Code>
    <WebBrowser Name="webBrowser" OpacityMask="Black" MaxHeight="650" MaxWidth="550"></WebBrowser>
</Window>
